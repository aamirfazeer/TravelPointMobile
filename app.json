{
  "expo": {
    "name": "TravelPoint",
    "scheme": ["travelpoint"],
    "slug": "travelpoint",
    "version": "1.0.0",
    "orientation": "portrait",
    "newArchEnabled": true,
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/travelpoint_image.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
    "plugins":[[
      "expo-build-properties",{
        "android": {
          "newArchEnabled":true
      },
      "ios": {
        "newArchEnabled": true
      }
    }
    ]]
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
